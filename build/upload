rsync -avz -e 'ssh -p 22' --del artifacts/jahshaka-auth-server/ root@192.241.172.251:/var/www/aspnet/Jahshaka.AuthServer
rsync -avz -e 'ssh -p 22' --del artifacts/jahshaka-web/ root@192.241.172.251:/var/www/aspnet/Jahshaka.Web

ssh -p 22 root@192.241.172.251 "supervisorctl restart jahshaka-auth-server; tail -vf -n 100 /var/log/jahshaka-auth-server.out.log;"
ssh -p 22 root@192.241.172.251 "supervisorctl restart jahshaka-web; tail -vf -n 100 /var/log/jahshaka-web.out.log;"

